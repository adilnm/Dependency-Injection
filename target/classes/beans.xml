<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
   
	<bean id="student" class="com.adil.dependancyinjection.Student">   
		<constructor-arg name="studentName" value="David"></constructor-arg> 
		<constructor-arg name="id" value="1234"></constructor-arg>
	</bean>  
	
	<bean id="std" class="com.adil.dependancyinjection.Student">   
		<constructor-arg name="id" value="5678"></constructor-arg>
	</bean> 
	

  </beans>
